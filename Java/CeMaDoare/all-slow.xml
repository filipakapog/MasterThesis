<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name = "all-slow" object-factory="org.powermock.modules.testng.PowerMockObjectFactory">
    <test name = "all-slow">
        <groups>
            <run>
                <include name="SLOW"></include>
            </run>
        </groups>
        <packages>
            <package name="cemadoare.*"></package>
        </packages>
    </test>
</suite>